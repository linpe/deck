#!/bin/bash
[[ -d static ]] && rm -r static;

mkdir "static";
mkdir "static/css";
mkdir "static/js";
mkdir "static/media";

cp -r "../save/build/static/media" "static/";

cp "../manager/build/static/css/"*.css "static/css/";
cp "../manager/build/static/js/"*.js "static/js/";
cp "../manager/build/index.html" "manager.html";

cp "../save/build/static/css/"*.css "static/css/";
cp "../save/build/static/js/"*.js "static/js/";
cp "../save/build/index.html" "save.html";
